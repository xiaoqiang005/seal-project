{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "docs": "jsdoc -c jsdoc.json",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "test": "vitest",
    "test:watch": "jest --watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress run",
    "test:e2e:dev": "cypress open",
    "test:component": "cypress run --component",
    "test:component:dev": "cypress open --component",
    "test:all": "node ../scripts/run-tests.js",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "pinia": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@cypress/vue": "^6.0.2",
    "@element-plus/icons-vue": "^2.3.1",
    "@types/jest": "^29.5.14",
    "@types/jsdoc-to-markdown": "^7.0.6",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^3.1.2",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.7.0",
    "@vue/vue3-jest": "^29.2.6",
    "@vuepress/bundler-vite": "^2.0.0-rc.21",
    "@vuepress/client": "^2.0.0-rc.21",
    "@vuepress/theme-default": "^2.0.0-rc.96",
    "babel-jest": "^29.7.0",
    "cypress": "^14.3.2",
    "docdash": "^2.0.2",
    "element-plus": "^2.9.8",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdoc": "^4.0.2",
    "jsdoc-plugin-typescript": "^3.2.0",
    "jsdoc-vuejs": "^4.0.0",
    "jsdom": "^26.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^3.1.2",
    "vue": "^3.5.13",
    "vue-tsc": "^1.8.25",
    "vuepress": "^2.0.0-rc.21"
  }
}
